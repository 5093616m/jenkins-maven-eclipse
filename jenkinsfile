pipeline{
	agent any
	tools {
		maven "maven3.9.6"
		jdk "OpenJDK11"
	}
	environment {
    PATH = "C:\\WINDOWS\\SYSTEM32"
	}
	stages{
		stage("clean"){
			steps{
			echo "Start Clean"
			bat "mvn clean"
			}
		  }
		stage("test"){
			steps{
			echo "Start Test"
			bat "mvn test"
			}
		  }
		stage("build"){
			steps{
			echo "Start build"
			bat "mvn install -DskipTests"
			}
		  }
		stage('SonarQube analysis') {
		    def scannerHome = tool 'SonarScanner 4.8.0.2856';
		    bat "${scannerHome}/bin/sonar-scanner"
		    }
		    }
          }
	}
}